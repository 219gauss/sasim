				0x03020004
				0x040201e7
				0x0003000b
				0x01030009
				0x02030006
				0x03030003
				0x040301e6
				0x0004020a
				0x01040008
				0x02040005
				0x03040002
				0x040401e5>;
		keypad,num-rows = <5>;
		keypad,num-columns = <5>;
